<view class="timeline ygobac">
  <view class="timeline-item" wx:for="{{list}}" wx:key="index">
    <view class="line"></view>
    <view class="circular"></view>
    <view class="item-body">
      <view class="title">{{item.dateString}}</view>
      <view class="content">{{item.content}}</view>
    </view>
  </view>
</view>