page,
view,form,input,button,textarea,picker,text,image {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

page {
  font: 24rpx/1 "SF Pro SC","SF Pro Text","SF Pro Icons","PingFang SC","Helvetica Neue","Helvetica","Arial",sans-serif;
  background-color: #e1e5ea;
}
label{
  display: inline-block;
  margin-bottom: 16rpx;
  line-height: 1.5;
}

.d-flex{
  display: flex!important;
}
.justify-content-center{
  justify-content: center;
}
.justify-content-end{
  justify-content: flex-end;
}
.justify-content-between{
  justify-content: space-between;
}
.align-items-center{
  align-items: center;
}
.align-items-end{
  align-items: flex-end;
}
.align-content-between{
  align-content: space-between;
}
.text-center{
  text-align: center;
}
.flex-wrap{
  flex-wrap: wrap;
}
.w-100{
  width: 100%;
}


.col-1 {
  flex: 0 0 8.33%;
  max-width: 8.33%;
}

.col-2 {
  flex: 0 0 16.66%;
  max-width: 16.66%;
}


.col-3 {
  flex: 0 0 25%;
  max-width: 25%;
}

.col-4 {
  flex: 0 0 33.33%;
  max-width: 33.33%;
}

.col-6 {
  flex: 0 0 50%;
  max-width: 50%;
}

.col-9 {
  flex: 0 0 75%;
  max-width: 75%;
}


.col-10 {
  flex: 0 0 83.33%;
  max-width: 83.33%;
}

.col-11 {
  flex: 0 0 91.66%;
  max-width: 91.66%;
}

.col-1,
.col-2,
.col-3,
.col-4,
.col-6,.col-9,.col-10,.col-11 {
  width: 100%;
}
.position-relative{
  position: relative;
}