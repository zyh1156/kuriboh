page,
view,
form,
input,
button,
textarea,
picker,
text,
image {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

page,
input,
textarea {
  font: 24rpx/1 system-ui,-apple-system,Segoe UI,Roboto,Ubuntu,Cantarell,Noto Sans,sans-serif;
  background-color: #e1e5ea;
}

label {
  display: inline-block;
  margin-bottom: 16rpx;
  line-height: 1.5;
}

.d-flex {
  display: flex !important;
}

.justify-content-center {
  justify-content: center;
}

.justify-content-end {
  justify-content: flex-end;
}

.justify-content-between {
  justify-content: space-between;
}

.align-items-center {
  align-items: center;
}

.align-items-end {
  align-items: flex-end;
}

.align-content-between {
  align-content: space-between;
}

.text-center {
  text-align: center;
}

.flex-wrap {
  flex-wrap: wrap;
}

.w-100 {
  width: 100%;
}

.position-relative {
  position: relative;
}
